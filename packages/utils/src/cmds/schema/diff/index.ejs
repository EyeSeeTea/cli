<!DOCTYPE html>
 <meta charset="UTF-8">
<html>
    <head>
        <link rel="stylesheet" href="data:text/css;base64,<%= jsondiffpatchCSS %>">

        <style>
            th {
                text-align: left
            }
            .left {
                background-color: #fbb
            }
            .right {
                background-color: #bfb
            }
        </style>
    </head>
    <body>
        <div>
             <div id="info">
                <div>
                    <table style="width:100%">
                    <tr>
                        <th></th>
                        <th class='left'>Left</th>
                        <th class='right'>Right</th>
                    </tr>
                    <% const metaPropsToShow = ['contextPath', 'version', 'revision', 'buildTime']%>
                    <% metaPropsToShow.forEach(key => { %>
                        <tr>
                            <td><%= key %></td>
                            <td><%= meta.left[key] %></td>
                            <td><%= meta.right[key] %></td>
                        </tr>
                    <%}) %>
                    </table>
                </div>
            </div>
            <div id="visual">
                <%- formatted%>
            </div>
        </div>
    </body>
</html>
